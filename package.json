{
  "name": "your-project-name",
  "version": "1.0.0",
  "description": "A Cypress testing project",
  "scripts": {
    "test": "cypress run",
    "test:ui": "cypress open"
  },
  "devDependencies": {
    "cypress": "13.15.1",
    "mocha-junit-reporter": "^2.0.0",
    "mocha-multi-reporters": "^1.0.0",
    "cypress-multi-reporters": "^1.0.0",
    "cypress-mochawesome-reporter": "^3.2.3"
  },
  "cypress": {
    "reporter": "cypress-mochawesome-reporter",
    "reporterOptions": {
      "reportDir": "cypress/results/mochawesome-report",
      "overwrite": false,
      "html": true,
      "json": true
    }
  }
}
